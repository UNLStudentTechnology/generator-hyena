{<% var ngVer = "1.3.0" %>
  "name": "<%= _.slugify(_.humanize(appname)) %>",
  "version": "0.0.0",
  "dependencies": {
    "angular": "^<%= ngVer %>",
    "json3": "^3.3.0",
    "es5-shim": "^4.0.0",
    "angular-animate": "^<%= ngVer %>"<% if (ariaModule) { %>,
    "angular-aria": "^<%= ngVer %>"<% } %><% if (cookiesModule) { %>,
    "angular-cookies": "^<%= ngVer %>"<% } %><% if (messagesModule) { %>,
    "angular-messages": "^<%= ngVer %>"<% } %><% if (resourceModule) { %>,
    "angular-resource": "^<%= ngVer %>"<% } %><% if (sanitizeModule) { %>,
    "angular-sanitize": "^<%= ngVer %>"<% } %><% if (touchModule) { %>,
    "angular-touch": "^<%= ngVer %>"<% } %>,
    "angular-ui-router": "~0.2.13",
    "firebase": "~2.1.1",
    "angularfire": "~0.9.0",
    "moment": "~2.8.3",
    "angular-moment": "~0.8.2",
    "ngstorage": "~0.3.0",
    "angular-messages": "~1.3.8",
    "ng-tags-input": "~2.1.1",
    "unl-style": "~0.1.0",
    "unl-components": "~0.1.0",
    "hyena-angular": "~0.1.0"
  },
  "devDependencies": {
    "angular-mocks": "^<%= ngVer %>",
    "angular-scenario": "^<%= ngVer %>"
  }<% if (appPath) { %>,
  "appPath": "<%= appPath %>"<% } %>
}
